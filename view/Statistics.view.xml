<mvc:View
	controllerName="main.controller.Statistics"
	xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<layout:Splitter width="100%" height="100%">
		<layout:contentAreas>			
			<ChartContainer
				id="chartContainer"
				showFullScreen="false"
				showLegend="false"
				showLegendButton="false"
				showPersonalization="false"
				showZoom="false"
				autoAdjustHeight="true"
				personalizationPress="attachPersonalizationPress"
				contentChange="attachContentChange">
				<toolbar>
					<m:OverflowToolbar id="appToolbar">
						<m:Button id="homeButton" icon="sap-icon://nav-back" tooltip="Вернуться" press="onPressHome"></m:Button>
						<m:ToolbarSpacer/>
						<ChartContainerToolbarPlaceholder></ChartContainerToolbarPlaceholder>
					</m:OverflowToolbar>
				</toolbar>				
			</ChartContainer>
		</layout:contentAreas>
	</layout:Splitter>
</mvc:View>